{
  "kind": "collectionType",
  "collectionName": "shoppingLists",
  "info": {
    "name": "shoppingList"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "shoppingListItems": {
      "collection": "shopping-list-item",
